{
  "build": {
    "tasks_filename": "build.json"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "game"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 10
      }
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    }
  },
  "managed": {
    "linux": {
      "artifact_name": "game@Linux.zip",
      "command": "./game",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    },
    "windows": {
      "artifact_name": "game@Windows.zip",
      "command": "./game.exe",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    },
    "macos": {
      "artifact_name": "game@Mac.zip",
      "command": "./game",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    }
  },
  "external": {
    "local": {
      "run_type": "EXECUTABLE_ZIP",
      "linux": {
        "artifact_name": "game@Linux.zip",
        "command": "./game",
        "arguments": [
          "localhost",
          "7777"
        ]
      },
      "windows": {
        "artifact_name": "game@Windows.zip",
        "command": "./game.exe",
        "arguments": [
          "localhost",
          "7777"
        ]
      },
      "macos": {
        "artifact_name": "game@Mac.zip",
        "command": "./game",
        "arguments": [
          "localhost",
          "7777"
        ]
      }
    }
  }
}